{
  "compilerOptions": {
        "lib": ["ES2021", "DOM"],
    "target": "ES2021",
    "module": "commonjs",
    "strictNullChecks": true,
    "strict": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./src/@types"]
  },
  "include": ["src"],
  "exclude": ["dist"]
}
